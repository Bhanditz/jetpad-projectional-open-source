<!DOCTYPE module
    PUBLIC
    "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
    "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd"
    >
<module>
  <source path="">
    <exclude name="ConceptDemoMain.java" />
    <exclude name="ExpressionDemoMain.java" />
    <exclude name="ContainerDemoMain.java" />
    <exclude name="HybridExprDemoMain.java" />
    <exclude name="IndentDemoMain.java" />
    <exclude name="diagram/DiagramDemoMain.java" />
    <exclude name="diagramExpr/DiagramDemoMain.java" />
  </source>

  <entry-point class="jetbrains.jetpad.projectional.demo.ProjectionalMain" />

  <inherits name="jetbrains.jetpad.model.Model" />
  <inherits name="jetbrains.jetpad.mapper.Mapper" />
  <inherits name="jetbrains.jetpad.test.Test" />
  <inherits name="com.google.gwt.query.Query"/>
  <inherits name="jetbrains.jetpad.projectional.view.View" />

  <inherits name="org.vectomatic.libgwtsvg" />

  <inherits name="jetbrains.jetpad.projectional.Projectional"/>
  <inherits name="jetbrains.jetpad.hybrid.Hybrid"/>
  <inherits name="jetbrains.jetpad.projectional.diagram.Diagram"/>

  <add-linker name="xsiframe"/>
  <set-configuration-property name="devModeRedirectEnabled" value="true"/>
  <set-configuration-property name="CssResource.style" value="pretty"/>
</module>
